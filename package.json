{
  "name": "davepeachvoice-payload-next",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "npm run build:payload && next build",
    "start": "next start",
    "build:payload": "cross-env PAYLOAD_CONFIG_PATH=lib/payload/payload.config.ts payload build && rm -rf public/admin && mv build public/admin",
    "copyfiles": "copyfiles -u 1 \"src/**/*.{html,css,scss,ttf,woff,woff2,eot,svg,jpg,png}\" dist/",
    "generate:types": "cross-env PAYLOAD_CONFIG_PATH=lib/payload/payload.config.ts payload generate:types",
    "generate:graphQLSchema": "PAYLOAD_CONFIG_PATH=lib/payload/payload.config.ts payload generate:graphQLSchema",
    "postinstall": "patch-package",
    "format": "prettier --write ."
  },
  "dependencies": {
    "@next/font": "^13.0.3",
    "@types/node": "18.11.9",
    "@types/react": "18.0.25",
    "@types/react-dom": "18.0.8",
    "classnames": "^2.3.2",
    "cloudinary-build-url": "^0.2.4",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "next": "^13.0.3",
    "payload": "1.1.18",
    "payload-cloudinary-plugin": "^0.0.9",
    "raw-loader": "^4.0.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-google-recaptcha": "^2.1.0",
    "react-icons": "^4.6.0",
    "react-player": "^2.11.0",
    "server-only": "^0.0.1",
    "wavesurfer-react": "^2.2.2"
  },
  "devDependencies": {
    "@cld-apis/types": "^0.1.6",
    "@types/express": "^4.17.14",
    "@types/node": "^18.11.8",
    "@types/react": "^18.0.24",
    "@types/react-google-recaptcha": "^2.1.5",
    "@types/wavesurfer.js": "^6.0.3",
    "@types/webpack-env": "^1.18.0",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "autoprefixer": "^10.4.13",
    "eslint": "^8.26.0",
    "eslint-config-next": "^13.0.3",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-webpack": "^0.13.2",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "frontmatter-markdown-loader": "^3.7.0",
    "patch-package": "^6.5.0",
    "postcss": "^8.4.19",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.8.4",
    "vercel": "^28.4.17"
  }
}
